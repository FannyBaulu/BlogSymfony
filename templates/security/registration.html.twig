{% extends 'base.html.twig' %}

{% block body %}

    <div class="m-3 d-flex justify-content-center">
        {{form_start(form)}}
            {{form_errors(form)}}
            {{form_row(form.email)}}
            {{form_row(form.pseudo)}}
            {{form_row(form.plainPassword)}}
            <button type="submit" class="mt-2 btn btn-outline-primary">S'inscrire</button>
        {{form_end(form)}}
    </div>

{% endblock %}